{
  "name": "tp-es",
  "version": "1.0.0",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:frontend": "npm --workspace apps/frontend run dev",
    "dev:backend": "npm --workspace apps/backend run dev",
    "cz": "cz",
    "format": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed:backend": "npm --workspace apps/backend exec ts-node src/scripts/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomneuenschwander/tp-es.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/thomneuenschwander/tp-es/issues"
  },
  "homepage": "https://github.com/thomneuenschwander/tp-es#readme",
  "devDependencies": {
    "@types/leaflet": "^1.9.17",
    "commitizen": "^4.3.1",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.20.6",
    "axios": "^1.9.0",
    "cookie": "^1.0.2",
    "cors": "^2.8.5",
    "dom-helpers": "^5.2.1",
    "is-core-module": "^2.16.1",
    "leaflet": "^1.9.4",
    "react-leaflet": "^5.0.0",
    "set-cookie-parser": "^2.7.1"
  }
}
